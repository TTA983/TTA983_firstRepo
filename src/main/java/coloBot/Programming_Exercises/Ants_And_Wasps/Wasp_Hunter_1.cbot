extern void object::WaspHunter1()
{
	object item;
	
	while (true)
	{
		while (radar(AlienWasp, 0, 360, 0, 20) == null)
		{
			item = radar(AlienWasp);
			turn(direction(item.position));
			motor(1,1);
			
			jet(0);
			if (position.z-topo(position) < 6)
			{
				jet(1);
			}
			
			if (position.z-topo(position) > 9)
			{
				jet(-1);
			}
			wait(0.2);
		}
		fire(1);
	}
	
}